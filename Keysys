local arrayTable = {"1234"}
wait(5)
for i = 1,#arrayTable do
print(arrayTable[i]
    if KeyEncryptionBox.Text == arrayTable[i] then
    	for i, Stuff in pairs(Encryption.BackgroundFrame:GetChildren()) do
			Stuff.Visible = false
		end
		Verifying.Visible = true
		repeat
			Verifying.Text = "Verifying."
			wait(1)
			Verifying.Text = "Verifying.."
			wait(1)
			Verifying.Text = "Verifying..."
		until wait(6)
		Verifying.Visible = false
		print("Enter")
		SlideIn:TweenSize(UDim2.new(0, 3, 0, 100),"InOut","Quad")
		SlideIn.Visible = false
		FrameBlur.Enabled = false
		Hub.Visible = true
		Hub:TweenPosition(UDim2.new(0.229, 0, 0.193, 0),"In","Quad")
   else
     print("Get a Key my slime")
    end
end
	
	
